{:deps
 {org.clojure/core.async      {:mvn/version "0.4.490"}
  com.cognitect.aws/api       {:mvn/version "0.8.305"}
  com.cognitect.aws/endpoints {:mvn/version "1.1.11.559"}

  com.cognitect.aws/rds       {:mvn/version "719.2.461.0"}

  ;; for STS refresh
  com.cognitect.aws/iam       {:mvn/version "718.2.450.0"}
  com.cognitect.aws/sts       {:mvn/version "718.2.453.0"}

  org.clojure/algo.monads     {:mvn/version "0.1.6"}

  ;; logging
  org.clojure/tools.logging   {:mvn/version "0.5.0-alpha.1"}
  log4j/log4j {:mvn/version"1.2.17"
               :exclusions [javax.mail/mail
                            javax.jms/jms
                            com.sun.jmdk/jmxtools
                            com.sun.jmx/jmxri]}

  ;; command line
  org.clojure/tools.cli       {:mvn/version "0.4.2"}
  }
 :paths ["src" "resources"]
 :aliases
 {;; clj -Aoutdated
  :outdated {:extra-deps {olical/depot {:mvn/version "RELEASE"}}
             :main-opts ["-m" "depot.outdated.main"]}

  ;; clj -Atest
  :test {:extra-paths ["test"]
         :extra-deps {com.cognitect/test-runner {:git/url "https://github.com/cognitect-labs/test-runner.git"
                                                 :sha "cb96e80f6f3d3b307c59cbeb49bb0dcb3a2a780b"}}
         :main-opts ["-m" "cognitect.test-runner"]}}}
